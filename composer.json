{
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true,
      "helhum/typo3-console-plugin": true
    }
  },
  "description": "TYPO3 CMS Base Distribution",
  "extra": {
    "helhum/typo3-console": {
      "active-packages": [
        "container"
      ],
      "comment": "This option is not needed ay more for helhum/typo3-console 5.x",
      "install-extension-dummy": false
    },
    "typo3/cms": {
      "web-dir": "public"
    }
  },
  "license": "GPL-2.0-or-later",
  "minimum-stability": "stable",
  "name": "typo3/cms-base-distribution",
  "repositories": [
    {
      "type": "path",
      "url": "./extensions/*"
    },
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    }
  ],
  "require": {
    "beechit/fal-securedownload": ">=2",
    "causal/file_list": ">=2",
    "cggrafing/user-template": "@dev",
    "evoweb/recaptcha": ">=9",
    "evoweb/sf-register": ">=9",
    "friendsoftypo3/typo3db-legacy": ">=1.1",
    "georgringer/news": ">=7",
    "helhum/typo3-console": ">=5",
    "jambagecom/static-info-tables-de": ">=6",
    "jambagecom/addons-em": "*",
    "sjbr/static-info-tables": ">=6",
    "typo3/cms-about": ">=9.5",
    "typo3/cms-adminpanel": ">=9.5",
    "typo3/cms-backend": ">=9.5",
    "typo3/cms-belog": ">=9.5",
    "typo3/cms-beuser": ">=9.5",
    "typo3/cms-context-help": ">=9.5",
    "typo3/cms-core": ">=9.5",
    "typo3/cms-cshmanual": ">=9.5",
    "typo3/cms-extbase": ">=9.5",
    "typo3/cms-extensionmanager": ">=9.5",
    "typo3/cms-felogin": ">=9.5",
    "typo3/cms-filelist": ">=9.5",
    "typo3/cms-fluid": ">=9.5",
    "typo3/cms-fluid-styled-content": ">=9.5",
    "typo3/cms-frontend": ">=9.5",
    "typo3/cms-impexp": ">=9.5",
    "typo3/cms-info": ">=9.5",
    "typo3/cms-install": ">=9.5",
    "typo3/cms-lang": ">=9.5",
    "typo3/cms-lowlevel": ">=9.5",
    "typo3/cms-recordlist": ">=9.5",
    "typo3/cms-reports": ">=9.5",
    "typo3/cms-rte-ckeditor": ">=9.5",
    "typo3/cms-seo": ">=9.5",
    "typo3/cms-setup": ">=9.5",
    "typo3/cms-t3editor": ">=9.5",
    "typo3/cms-tstemplate": ">=9.5",
    "typo3/cms-viewpage": ">=9.5",
    "sbublies/gridtocontainer": "^11.4",
    "b13/container": "^2.2",
    "buepro/typo3-container-elements": "^4.0"
  },
  "require-dev": {
    "doctrine/common": ">=3.3",
    "roave/security-advisories": "dev-latest",
    "symfony/var-dumper": ">=5.4",
    "typo3/fluid-schema-generator": ">=2.1",
    "rector/rector": "^0.15.20",
    "ssch/typo3-rector": "^1.1"
  },
  "scripts": {
    "post-autoload-dump": [
      "@typo3-cms-scripts"
    ],
    "typo3-cms-scripts": [
      "@php vendor/bin/typo3cms install:fixfolderstructure"
    ]
  }
}
